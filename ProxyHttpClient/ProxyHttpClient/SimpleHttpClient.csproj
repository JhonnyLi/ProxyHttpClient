  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="copy C:\Users\jhonn\.nuget\packages\newtonsoft.json\12.0.1\lib\netstandard2.0\*.dll $(OutDir)" />
  </Target>

</Project>
